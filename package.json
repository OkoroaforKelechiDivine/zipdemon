{  
  "name": "zipdemon",  
  "displayName": "ZipDemon",  
  "description": "A VS Code extension that automatically generates documentation.",  
  "version": "0.0.1",  
  "engines": {  
    "vscode": "^1.54.0"  
  },  
  "activationEvents": [  
    "onCommand:zipdemon.showPlayButton"  
  ],  
  "contributes": {  
    "commands": [  
      {  
        "command": "zipdemon.showPlayButton",  
        "title": "Show Play Button"  
      },  
      {  
        "command": "zipdemon.helloWorld",  
        "title": "Hello World"  
      },  
      {  
        "command": "zipdemon.generateDocumentation",  
        "title": "Generate Documentation"  
      }  
    ]  
  },  
  "main": "./extension.js",  
  "scripts": {  
    "lint": "eslint .",  
    "pretest": "npm run lint",  
    "test": "vscode-test"  
  },  
  "devDependencies": {  
    "@types/vscode": "^1.54.0",  
    "@types/mocha": "^10.0.10",  
    "@types/node": "20.x",  
    "eslint": "^9.16.0",  
    "@vscode/test-cli": "^0.0.10",  
    "@vscode/test-electron": "^2.4.1"  
  }  
}